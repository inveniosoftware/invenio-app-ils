{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "address": {
      "title": "The address of vendor",
      "type": "string"
    },
    "email": {
      "title": "The email of vendor",
      "type": "string"
    },
    "legacy_id": {
      "type": "string",
      "title": "The previous id of this Vendor before data migration"
    },
    "name": {
      "title": "The name of vendor",
      "type": "string"
    },
    "notes": {
      "title": "The notes of vendor",
      "type": "string"
    },
    "phone": {
      "title": "The phone of Vendor",
      "type": "string"
    },
    "pid": {
      "title": "The persistent identifier of vendor",
      "type": "string"
    }
  },
  "required": ["$schema", "pid", "name"],
  "title": "Schema for Acquisition Vendor",
  "type": "object"
}
