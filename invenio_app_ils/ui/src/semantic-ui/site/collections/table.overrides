/*******************************
         Site Overrides
*******************************/

@{fs-parent-selector}{
  .ui.table{
    border: 0;
    tr{
      td{
        border: 0;
      }
    }
    &.definition{
      td:nth-child(2){
        border-left: 0;

      }
      tr {
        td:first-child:not(.ignored){
          background: inherit;
        }
      }
    }
  }
}
