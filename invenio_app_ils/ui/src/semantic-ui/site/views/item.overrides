/*******************************
    User Variable Overrides
*******************************/


@{fs-parent-selector} {
  .ui.items {
    &.divided {
      :not(.ui.list) {
      .item {
        padding: 1em;
        &:first-child{
          padding-top: 1em !important;
        }
      }
    }

    }
  }

  .item {
    &.bkg-danger {
      background-color: lighten(@danger, 40%) !important;
    }
  }
}
