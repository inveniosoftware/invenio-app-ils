    User Variable Overrides
*******************************/
.ui.list {
  &.horizontal{
  &.document-authors-list, &.document-languages-list {
    .item {
      margin-left: 0.5rem;
    }

    .item:before {
      content: ' '
    }
  }
}
}

.ui.container.search-body {
  .ui.card {
    .ui.list {
      &.horizontal.document-authors-list {
        .item {
          margin-left: 0.1rem;
        }
      }
    }
  }
}

