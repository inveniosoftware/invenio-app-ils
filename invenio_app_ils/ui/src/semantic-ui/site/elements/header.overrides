/*******************************
         Site Overrides
*******************************/

.ui.container.footer-upper .ui.header{
    color: @secondaryColor;
}

.ui.container.footer-lower .ui.header{
    color: #c1bbbb;
}

.footer-upper .ui.header .sub.header{
    color: #9e9ea0;
}

.backoffice {

  .ui.header.bo-menu-header {
    margin-top: 1em;
    margin-bottom: 2em;

    .icon {
      font-size: 2em;
    }
    .sub.header {
      text-transform: uppercase;
    }
  }
}
