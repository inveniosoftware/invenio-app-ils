/*******************************
         Site Overrides
*******************************/

.ui.label.highlighted{
  border: 1px solid @fsPrimaryColor;
  background-color: white;
  color: @fsPrimaryColor;
  a{
    color: @fsPrimaryColor;
    opacity: 1;
    &:hover{
      color: @linkHoverColor;
    }
  }
}
