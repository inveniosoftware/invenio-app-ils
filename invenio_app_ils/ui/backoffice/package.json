{
  "name": "backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "move-js": "rm build/static/js/runtime~main.*.js* && cpr build/static/js/ ../../static/js/invenio_app_ils/backoffice/ -d -f map",
    "move-css": "cpr build/static/css/ ../../static/css/invenio_app_ils/backoffice/ -d -f map",
    "pre-build": "react-scripts build",
    "build": "npm-run-all pre-build move-js move-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "devDependencies": {
    "D": "^1.0.0",
    "cpr": "^3.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jsonfile": "^4.0.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "redux-devtools-extension": "^2.13.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
