{
  "name": "backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "move-js": "rm build/static/js/runtime~main.*.js* && cpr build/static/js/ ../../static/js/invenio_app_ils/backoffice/ -d -f map",
    "move-css": "cpr build/static/css/ ../../static/css/invenio_app_ils/backoffice/ -d -f map",
    "pre-build": "react-scripts build",
    "build": "npm-run-all pre-build move-js move-css",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "luxon": "^1.7.1",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-searchkit": "0.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "D": "^1.0.0",
    "axios-mock-adapter": "^1.15.0",
    "cpr": "^3.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.1.3",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.1",
    "pretty-quick": "^1.6.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
