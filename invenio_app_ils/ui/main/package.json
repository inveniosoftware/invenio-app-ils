{
  "name": "main",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "react-scripts start",
    "start-server": "nodemon ./server/index.js",
    "build-data": "node ./server/mocks/builder.js",
    "start": "npm-run-all -p start-js start-server",
    "move-js": "rm build/static/js/runtime~main.*.js* && cpr build/static/js/ ../../static/js/invenio_app_ils/main/ -d -f map",
    "move-css": "cpr build/static/css/ ../../static/css/invenio_app_ils/main/ -d -f map",
    "move-media": "cpr build/static/media ../../static/media -o",
    "move-images": "cpr build/static/images ../../static/images -o",
    "pre-build": "react-scripts build",
    "build": "npm-run-all pre-build move-js move-css move-media move-images",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.2"
  },
  "devDependencies": {
    "D": "^1.0.0",
    "cpr": "^3.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "jsonfile": "^4.0.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "redux-devtools-extension": "^2.13.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
