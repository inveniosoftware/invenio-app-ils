                  "INSPIRE",
                  "INSPIRE-CNUM",
                  "KEK",
                  "LHCLHC",
                  "SAFARI",
                  "SCEM",
                  "UDCCERN",
                  "WAI01"
                ],
                "minLength": 1,
                "pattern": "^\\w+$",
                "type": "string"
              },
              "value": {
                "minLength": 1,
                "pattern": "^\\S+$",
                "title": "External identifier",
                "type": "string"
              }
            },
            "required": ["schema", "value"],
            "type": "object"
          },
          "minItems": 1,
          "type": "array",
          "uniqueItems": true
        },
        "isbns": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "description": {
                "minLength": 1,
                "title": "Free text, used to describe the document to ISBN refers to.",
                "type": "string"
              },
              "medium": {
                "enum": [
                  "audiobook",
                  "CD-ROM",
                  "DVD",
                  "electronic version",
                  "print version",
                  "print version, hardback",
                  "print version, paperback",
                  "print version, spiral-bound"
                ],
                "minLength": 1,
                "title": "Physical medium to which this ISBN refers",
                "type": "string"
              },
              "value": {
                "minLength": 1,
                "pattern": "^\\d*[0-9X]$",
                "type": "string"
              }
            },
            "required": ["value"],
            "type": "object"
          },
          "minItems": 1,
          "title": "List of ISBNs",
          "type": "array",
          "uniqueItems": true
        },
        "persistent_identifiers": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "material": {
                "enum": [
                  "addendum",
                  "additional material",
                  "data",
                  "e-book",
                  "e-proceedings",
                  "editorial note",
                  "erratum",
                  "preprint",
                  "publication",
                  "reprint",
                  "software",
                  "translation"
                ],
                "minLength": 1,
                "title": "Material to which the field refers",
                "type": "string"
              },
              "schema": {
                "minLength": 1,
                "title": "Type of identifier",
                "type": "string"
              },
              "source": {
                "minLength": 1,
                "type": "string"
              },
              "value": {
                "minLength": 1,
                "pattern": "^\\S+$",
                "title": "Persistent identifier",
                "type": "string"
              }
            },
            "required": ["schema", "value"],
            "type": "object"
          },
          "minItems": 1,
          "title": "List of persistent identifiers",
          "type": "array",
          "uniqueItems": true
        },
        "report_numbers": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "hidden": {
                "description": "Used in cases when report number is not yet officially assigned.",
                "type": "boolean"
              },
              "source": {
                "minLength": 1,
                "type": "string"
              },
              "value": {
                "minLength": 1,
                "type": "string"
              }
            },
            "required": ["value"],
            "type": "object"
          },
          "minItems": 1,
          "title": "List of report numbers assigned to the record",
          "type": "array",
          "uniqueItems": true
        },
        "standard_numbers": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "hidden": {
                "type": "boolean"
              },
              "value": {
                "minLength": 1,
                "type": "string"
              }
            },
            "required": ["value"],
            "type": "object"
          },
          "minItems": 1,
          "title": "List of standard numbers",
          "type": "array",
          "uniqueItems": true
        }
      },
      "title": "Document identifiers.",
      "type": "object"
    },
    "imprints": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "date": {
            "format": "date",
            "minLength": 1,
            "title": "Date of publication",
