/*******************************
         Site Overrides
*******************************/

.ui.label.highlighted {
  border: 1px solid @fsPrimaryColor;
  background-color: white;
  color: @fsPrimaryColor;
  a {
    color: @fsPrimaryColor;
    opacity: 1;
    &:hover {
      color: @linkHoverColor;
    }
  }
}

.ui.label.muted {
  border: 0;
  padding: 0;
  color: @mutedTextColor;
  background-color: transparent;
}
